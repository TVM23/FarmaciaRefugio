    <script src="https://kit.fontawesome.com/85d9d2a581.js" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>


 </head>
<body>
    {{#if success_msg}}
        <script>
            Swal.fire({
            title: 'Registro completo',
            text: 'Su cuenta ha sido registrada con exito',
            icon: 'success',
            confirmButtonText: 'Ok'
          })
        </script>
    {{/if}}
    {{#if cuenta_repetida}}
        <script>
            Swal.fire({
            title: 'Atencion',
            text: 'Este correo ya esta registrado',
            icon: 'warning',
            confirmButtonText: 'Entendido'
          })
        </script>
    {{/if}}
    {{#if error}}
        <script>
            Swal.fire({
            title: 'Atencion',
            text: `{{error}}`,
            icon: 'error',
            confirmButtonText: 'Entendido'
          })
        </script>
    {{/if}}
    {{#if cerrar_sesion}}
        <script>
            Swal.fire({
            title: 'Atencion',
            text: 'Se ha cerrado la sesion con exito',
            icon: 'success',
            confirmButtonText: 'Entendido'
          })
        </script>
    {{/if}}
</body>
